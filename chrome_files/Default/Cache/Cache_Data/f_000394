        function goFition() {
            document.querySelectorAll('.snippet-parragraph').forEach((elt) => {
                const sqs = fitionDatum.filter(fitionData => fitionData.properties.type === 'square' || fitionData.properties.type === 'squarish');
                const sq =  sqs[Math.floor(Math.random() * sqs.length)];
                if (sq) {
                    fitionDatum = fitionDatum.filter(fit => fit !== sq);
                    const insElt = elt.querySelector('ins');
                    if (insElt) {
                        elt.removeChild(insElt);
                    }
                    const a = document.createElement('a');
                    a.href = sq.link;
                    a.style.display = 'inline-block';
                    a.style.float = 'left';
                    a.style.margin = '15px 25px 25px 0';
                    a.innerHTML = '<img style="max-width: 300px; max-height: 300px" src="' + sq.img + '" />';
                    elt.appendChild(a);
                }
            });

            const elt = document.querySelector('.content-column')
            if (elt) {
                const rm = elt.querySelector('.clearfix.intro-bottom.intro-top');
                if (rm) {
                    elt.removeChild(rm);
                }
                const hzs = fitionDatum.filter(fitionData => fitionData.properties.type === 'horizontal' || fitionData.properties.type === 'horizontal');
                if (hzs) {
                    const hz =  hzs[Math.floor(Math.random() * hzs.length)];
                    const div = document.createElement('div');
                    div.style.display = 'flex';
                    div.style['justify-content'] = 'center';
                    div.style['align-items'] = 'center';
                    const a = document.createElement('a');
                    a.href = hz.link;
                    a.style.margin = '15px 25px 25px 0';
                    a.innerHTML = '<img style="max-width: 100%; max-height: 100%; margin: 0 auto;" src="' + hz.img + '" />';
                    div.appendChild(a);
                    elt.prepend(div);
                }

            }
        }

        googlefc.callbackQueue.push({
            'AD_BLOCK_DATA_READY':
                () => {
                    switch (googlefc.getAdBlockerStatus()) {
                        case googlefc.AdBlockerStatusEnum.EXTENSION_LEVEL_AD_BLOCKER:
                        case googlefc.AdBlockerStatusEnum.NETWORK_LEVEL_AD_BLOCKER:
                            goFition();
                            break;

                    }

                }
        });
    
